<div class="container-fluid top-bar">
  <%= link_to "New Event", new_event_path, :class => 'btn primary'%>
</div>
<div class="container-fluid">
  <div class="sidebar whitebkg">
    <div id="calendar-selector">
      
    </div>
    <input type="hidden" id="times-available-hidden" name="times_available" value="<%= time_available_values %>">
    <div style="padding: 5px" id="events-list">
    
    </div>
  </div>
  <div class="content">
    <div id="time-table">
    </div>
    <h6>Notes</h6>
    <div id="notes-view" class="well">

    </div>
    <%= raw(calendar :year => Time.now.year, :month => Time.now.month) %>
  </div>
</div>
<div id="modal-from-dom" class="modal wide hide fade">
 
</div>